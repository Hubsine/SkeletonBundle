{
    "name": "hubsine/skeleton-bundle",
    "type": "symfony-bundle",
    "description": "Skeleton bundle for Symfony applications",
    "keywords": ["backend", "fontend", "admin"],
    "homepage": "https://github.com/Hubsine/SkeletonBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Hubsine",
            "homepage": "https://www.hubsine.com"
        }
    ],
    "require": {
        "php": "^7.0.8",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "a2lix/translation-form-bundle": "3.0.4",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "easycorp/easyadmin-bundle": "^1.17",
        "friendsofsymfony/ckeditor-bundle": "^2.1",
        "friendsofsymfony/user-bundle": "^2.1",
        "helios-ag/fm-elfinder-bundle": "^9.3",
        "knplabs/doctrine-behaviors": "~1.1",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/console": "3.4.*",
        "symfony/dotenv": "3.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "3.4.*",
        "symfony/swiftmailer-bundle": "^3.2@dev",
        "symfony/twig-bundle": "3.4.*",
        "symfony/validator": "3.4.*",
        "symfony/webpack-encore-bundle": "^1.6",
        "symfony/yaml": "3.4.*",
        "vich/uploader-bundle": "1.8.*"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.13",
        "symfony/profiler-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "component-dir": "public/components"
    },
    "autoload": {
        "psr-4": {
            "Hubsine\\SkeletonBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hubsine\\SkeletonBundle\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "ckeditor:install --clear=drop": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "require": "3.4.*"
        },
        "branch-alias": {
            "dev-master": "1.0"
        }
    }
}
